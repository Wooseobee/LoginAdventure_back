<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.la.user.model.security.dao.SecurityMapper">


    <insert id="insertSecurity" parameterType="com.ssafy.web.member.dto.SecDto">
        insert into openeg_security(userid,salt,seckey) values(#{userid}, #{salt}, #{seckey})
    </insert>

    <select id="readSalt" parameterType="String" resultType="String">
        select salt from openeg_security where userid=#{userid}
    </select>



</mapper>